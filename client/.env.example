# Client environment variables example
# Copy this file to .env and add your actual API keys

# Unsplash API key for trip images
REACT_APP_UNSPLASH_API_KEY=your_unsplash_api_key

# OpenWeatherMap API key for weather forecasts
REACT_APP_WEATHER_API_KEY=your_openweathermap_api_key

# OpenRouteService API key for route planning
REACT_APP_ORS_API_KEY=your_openrouteservice_api_key
